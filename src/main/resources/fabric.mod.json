{
  "schemaVersion": 1,
  "id": "emc-essentials",
  "version": "${version}",
    "name": "EarthMCEssentials",
  "description": "A mod providing info on the EarthMC Minecraft server.",
  "authors": [
    "Owen77Stubbs",
    "Warriorrr"
  ],
  "contact": {
    "homepage": "https://github.com/Warriorrrr/EarthMCEssentials",
    "issues": "https://github.com/Warriorrrr/EarthMCEssentials/issues"
  },

  "license": "CC0-1.0",
  "icon": "assets/emc/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.earthmc.emc.EMCMod"
    ],
    "modmenu": [
      "net.earthmc.emc.ModMenuIntegration"
    ],
    "cotton-client-commands": [
      "net.earthmc.emc.ClientSideCommands"
    ]
  },
  "mixins": [
    "emc.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.15.x"
  },
  "custom": {
    "modmenu:clientsideOnly": true
  }
}